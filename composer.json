{
  "name": "apsonex/spatial",
  "authors": [
    {
      "name": "Apsonex Inc.",
      "email": "apsonexinc@gmail.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/apsonex/spatial",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.8.2",
    "geo-io/wkb-parser": "^1.0.2",
    "jmikola/geojson": "^1.2.0"
  },
  "require-dev": {
    "laravel/framework": "^10.0|^11.0",
    "nunomaduro/collision": "^8.5",
    "orchestra/testbench": "^9.8",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "Apsonex\\Spatial\\": "src",
      "Apsonex\\Spatial\\Database\\Factories\\": "database/factories"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Apsonex\\Spatial\\Tests\\": "tests",
      "Apsonex\\Spatial\\Seeders\\": "seeders"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Apsonex\\Spatial\\SpatialServiceProvider"
      ]
    }
  },
  "repositories": [
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
